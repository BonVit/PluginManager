<manifest
        xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.vitaliibonar.pluginvibe">

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application tools:ignore="AllowBackup,GoogleAppIndexingWarning,MissingApplicationIcon">

        <meta-data android:name="PLUGIN_COMPONENT_NAME"
                   android:value="com.vitaliibonar.pluginvibe.PluginVibeService"/>

        <service android:name="com.vitaliibonar.pluginvibe.PluginVibeService">
            <intent-filter>
                <action android:name="com.vitaliibonar.pluginvibe.PluginVibeService"/>
            </intent-filter>
        </service>

    </application>
</manifest>
